func gemLock() {
    if expert.isOnGem {
        expert.collectGem()
    }
    expert.turnLockUp()
}


func goCross() {
    while !expert.isBlocked {
        expert.moveForward()
    }
    gemLock()
    expert.turnRight()
    expert.turnRight()
    for i in 1 ... 3 {
        expert.moveForward()
    }
    expert.turnLeft()
}

for i in 1 ... 4 {
    goCross()
}
